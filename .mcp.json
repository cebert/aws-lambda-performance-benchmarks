{
  "mcpServers": {
    "aws-cdk": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "awslabs.cdk-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    },
    "aws-docs": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_DOCUMENTATION_PARTITION": "aws"
      }
    },
    "awslabs-cdk-mcp-server": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "awslabs.cdk-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": ""
      }
    },
    "aws-core": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "awslabs.aws-core-mcp-server@latest"
      ],
      "env": {}
    }
  }
}